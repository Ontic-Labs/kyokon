{
  "openapi": "3.1.0",
  "info": {
    "title": "Kyokon API",
      "description": "REST API for USDA FoodData Central SR Legacy, Foundation, and Branded foods data.\n\n**Kyokon** (Â∑®Ê†π) ‚Äî it means exactly what you think it means. üçÜ ([Google Translate](https://translate.google.com/?sl=ja&tl=en&text=%E5%B7%A8%E6%A0%B9&op=translate))\n\nProvides access to **8,158 foods** with nutrients, portions, categories, and cookability data.\n\n---\n\n### Authentication\n\nAPI keys are simple strings configured on the server.\n\n**Usage (any of these methods):**\n- Query parameter: `?api_key=YOUR_KEY`\n- Header: `X-API-Key: YOUR_KEY`\n- Bearer token: `Authorization: Bearer YOUR_KEY`\n\n**Note:** If no API keys are configured on the server, the API is open.",
    "version": "1.8.0"
  },
  "servers": [
    {
      "url": "/api",
      "description": "Relative API base"
    }
  ],
  "security": [
    { "apiKey": [] },
    { "apiKeyHeader": [] },
    { "bearerAuth": [] }
  ],
  "tags": [
    { "name": "Foods", "description": "Food search and detail endpoints" },
    { "name": "Categories", "description": "Food category endpoints" },
    { "name": "Nutrients", "description": "Nutrient listing endpoints" },
    { "name": "Ingredients", "description": "Recipe-first canonical ingredient endpoints" },
    { "name": "Canonicals", "description": "Canonical ingredient aggregate endpoints (legacy)" }
  ],
  "paths": {
    "/foods": {
      "get": {
        "tags": ["Foods"],
        "summary": "Search and filter foods",
        "description": "Full-text search with optional filters for category, nutrient range, cooking state, preservation, processing, and cookability.",
        "operationId": "searchFoods",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Full-text search query (uses PostgreSQL plainto_tsquery)",
            "schema": { "type": "string" }
          },
          {
            "name": "categoryId",
            "in": "query",
            "description": "Filter by food category ID",
            "schema": { "type": "integer" }
          },
          {
            "name": "nutrientId",
            "in": "query",
            "description": "Filter by nutrient ID (requires nutrient to exist in food)",
            "schema": { "type": "integer" }
          },
          {
            "name": "min",
            "in": "query",
            "description": "Minimum nutrient amount (requires nutrientId)",
            "schema": { "type": "number" }
          },
          {
            "name": "max",
            "in": "query",
            "description": "Maximum nutrient amount (requires nutrientId)",
            "schema": { "type": "number" }
          },
          {
            "name": "cookable",
            "in": "query",
            "description": "Filter by cookability assessment",
            "schema": { "type": "boolean" }
          },
          {
            "name": "state",
            "in": "query",
            "description": "Cooking state filter",
            "schema": {
              "type": "string",
              "enum": ["unknown", "raw", "cooked"]
            }
          },
          {
            "name": "preservation",
            "in": "query",
            "description": "Preservation method filter",
            "schema": {
              "type": "string",
              "enum": ["unknown", "fresh", "frozen", "canned", "dried", "cured", "pickled", "fermented", "smoked", "shelf_stable"]
            }
          },
          {
            "name": "processing",
            "in": "query",
            "description": "Physical processing filter",
            "schema": {
              "type": "string",
              "enum": ["unknown", "whole", "ground", "sliced", "diced", "shredded", "pureed", "paste", "powder", "flour", "juice", "oil", "broth", "stock"]
            }
          },
          {
            "name": "canonicalSlug",
            "in": "query",
            "description": "Filter by canonical base slug (e.g. 'beer', 'chicken', 'agave')",
            "schema": { "type": "string" }
          },
          {
            "name": "page",
            "in": "query",
            "description": "Page number (1-indexed)",
            "schema": { "type": "integer", "default": 1, "minimum": 1 }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "Items per page",
            "schema": { "type": "integer", "default": 25, "minimum": 1, "maximum": 100 }
          }
        ],
        "responses": {
          "200": {
            "description": "Paginated list of foods",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PaginatedFoods"
                }
              }
            }
          },
          "400": {
            "description": "Invalid query parameters",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ErrorResponse" }
              }
            }
          }
        }
      }
    },
    "/foods/{fdcId}": {
      "get": {
        "tags": ["Foods"],
        "summary": "Get food detail",
        "description": "Returns detailed information for a single food. Use the `include` parameter to specify which related data to fetch.",
        "operationId": "getFoodDetail",
        "parameters": [
          {
            "name": "fdcId",
            "in": "path",
            "required": true,
            "description": "FDC ID of the food",
            "schema": { "type": "integer" }
          },
          {
            "name": "include",
            "in": "query",
            "description": "Comma-separated list of related data to include. Options: `nutrients`, `portions`. Defaults to both if omitted.",
            "schema": { "type": "string", "example": "nutrients,portions" }
          }
        ],
        "responses": {
          "200": {
            "description": "Food detail with optional nutrients and portions",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/FoodDetail" }
              }
            }
          },
          "404": {
            "description": "Food not found",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ErrorResponse" }
              }
            }
          }
        }
      }
    },
    "/categories": {
      "get": {
        "tags": ["Categories"],
        "summary": "List food categories",
        "description": "Returns all food categories, optionally with food counts.",
        "operationId": "getCategories",
        "parameters": [
          {
            "name": "includeCounts",
            "in": "query",
            "description": "Include count of foods per category",
            "schema": { "type": "boolean", "default": false }
          }
        ],
        "responses": {
          "200": {
            "description": "List of categories",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    { "$ref": "#/components/schemas/CategoriesResponse" },
                    { "$ref": "#/components/schemas/CategoriesWithCountResponse" }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/ingredients": {
      "get": {
        "tags": ["Ingredients"],
        "summary": "List canonical ingredients",
        "description": "Browse and search recipe-first canonical ingredients. Each ingredient represents a real recipe term (e.g. 'ground beef', 'salt') mapped to FDC foods with aggregated nutrient boundaries.",
        "operationId": "searchIngredients",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Case-insensitive partial match on ingredient name",
            "schema": { "type": "string" }
          },
          {
            "name": "hasNutrients",
            "in": "query",
            "description": "Filter to only ingredients with computed nutrient data",
            "schema": { "type": "boolean" }
          },
          {
            "name": "page",
            "in": "query",
            "schema": { "type": "integer", "default": 1, "minimum": 1 }
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": { "type": "integer", "default": 25, "minimum": 1, "maximum": 100 }
          }
        ],
        "responses": {
          "200": {
            "description": "Paginated list of canonical ingredients",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/PaginatedIngredients" }
              }
            }
          }
        }
      }
    },
    "/ingredients/{slug}": {
      "get": {
        "tags": ["Ingredients"],
        "summary": "Get ingredient detail with nutrient boundaries",
        "description": "Returns detailed information for a canonical ingredient, including statistical nutrient values (median, p10/p90, p25/p75 boundaries) computed from all mapped FDC foods.",
        "operationId": "getIngredientDetail",
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "description": "Canonical ingredient slug (e.g. 'ground-beef', 'salt', 'olive-oil')",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "Ingredient detail with nutrient boundaries",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/IngredientDetail" }
              }
            }
          },
          "404": {
            "description": "Ingredient not found",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ErrorResponse" }
              }
            }
          }
        }
      }
    },
    "/ingredients/resolve": {
      "post": {
        "tags": ["Ingredients"],
        "summary": "Resolve ingredient names to canonical ingredients",
        "description": "Accepts an array of free-text ingredient names (e.g. 'ground beef', 'kosher salt', '2 cups flour') and resolves each to its canonical ingredient with full nutrient boundaries. Resolution uses: direct slug match, alias lookup, then trigram fuzzy matching. Max 50 ingredients per request.",
        "operationId": "resolveIngredients",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["ingredients"],
                "properties": {
                  "ingredients": {
                    "type": "array",
                    "items": { "type": "string", "minLength": 1 },
                    "minItems": 1,
                    "maxItems": 50,
                    "example": ["ground beef", "kosher salt", "olive oil", "flour"]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Resolution results for each input ingredient",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ResolveResponse" }
              }
            }
          },
          "400": {
            "description": "Invalid request body",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ErrorResponse" }
              }
            }
          }
        }
      }
    },
    "/ingredients/export": {
      "get": {
        "tags": ["Ingredients"],
        "summary": "Export all ingredients as JSON",
        "description": "Downloads a complete JSON export of all canonical ingredients with their nutrient data. Returns a file with `Content-Disposition: attachment` header.",
        "operationId": "exportIngredients",
        "responses": {
          "200": {
            "description": "JSON file download containing all ingredients",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "required": ["exportedAt", "count", "ingredients"],
                  "properties": {
                    "exportedAt": { "type": "string", "format": "date-time" },
                    "count": { "type": "integer" },
                    "ingredients": { "type": "array", "items": { "$ref": "#/components/schemas/IngredientDetail" } }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/canonicals": {
      "get": {
        "tags": ["Canonicals"],
        "summary": "Search canonical aggregates",
        "description": "Browse and search canonical ingredient aggregates. Each aggregate represents a statistical composite of all foods sharing a canonical name.",
        "operationId": "searchCanonicals",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Case-insensitive partial match on canonical name",
            "schema": { "type": "string" }
          },
          {
            "name": "page",
            "in": "query",
            "schema": { "type": "integer", "default": 1, "minimum": 1 }
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": { "type": "integer", "default": 25, "minimum": 1, "maximum": 100 }
          }
        ],
        "responses": {
          "200": {
            "description": "Paginated list of canonical aggregates",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/PaginatedCanonicals" }
              }
            }
          }
        }
      }
    },
    "/canonicals/{slug}": {
      "get": {
        "tags": ["Canonicals"],
        "summary": "Get canonical aggregate detail",
        "description": "Returns detailed information for a canonical aggregate, including statistical nutrient values (median, p5/p95 boundaries) and source foods.",
        "operationId": "getCanonicalDetail",
        "parameters": [
          {
            "name": "slug",
            "in": "path",
            "required": true,
            "description": "Canonical slug (e.g. 'black-pepper', 'chicken')",
            "schema": { "type": "string" }
          }
        ],
        "responses": {
          "200": {
            "description": "Canonical aggregate with nutrients and sources",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/CanonicalDetail" }
              }
            }
          },
          "404": {
            "description": "Canonical aggregate not found",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/ErrorResponse" }
              }
            }
          }
        }
      }
    },
    "/nutrients": {
      "get": {
        "tags": ["Nutrients"],
        "summary": "List nutrients",
        "description": "Returns all nutrients with optional search filtering.",
        "operationId": "searchNutrients",
        "parameters": [
          {
            "name": "search",
            "in": "query",
            "description": "Case-insensitive partial match on nutrient name",
            "schema": { "type": "string" }
          },
          {
            "name": "page",
            "in": "query",
            "schema": { "type": "integer", "default": 1, "minimum": 1 }
          },
          {
            "name": "pageSize",
            "in": "query",
            "schema": { "type": "integer", "default": 25, "minimum": 1, "maximum": 100 }
          }
        ],
        "responses": {
          "200": {
            "description": "Paginated list of nutrients",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/PaginatedNutrients" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "in": "query",
        "name": "api_key",
        "description": "API key passed as query parameter (like USDA FDC). Example: `?api_key=demo`"
      },
      "apiKeyHeader": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key",
        "description": "API key passed via header. Example: `X-API-Key: demo`"
      },
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "description": "API key as Bearer token. Example: `Authorization: Bearer demo`"
      }
    },
    "schemas": {
      "FoodListItem": {
        "type": "object",
        "required": ["fdcId", "description", "categoryId"],
        "properties": {
          "fdcId": { "type": "integer", "description": "Unique FDC identifier" },
          "description": { "type": "string", "description": "Food name/description" },
          "categoryId": { "type": ["integer", "null"], "description": "Category ID" },
          "categoryName": { "type": "string", "description": "Category name" },
          "canonicalBaseName": { "type": "string", "description": "Canonical base name (broad identity)" },
          "canonicalBaseSlug": { "type": "string", "description": "URL-safe slug of canonical base name" },
          "canonicalSpecificName": { "type": "string", "description": "Canonical specific name (subtype)" },
          "canonicalSpecificSlug": { "type": "string", "description": "URL-safe slug of canonical specific name" },
          "cookingState": { "type": "string", "enum": ["unknown", "raw", "cooked"] },
          "cookingMethods": { "type": "array", "items": { "type": "string" } },
          "preservation": { "type": "string" },
          "processing": { "type": "string" }
        }
      },
      "FoodDetail": {
        "type": "object",
        "required": ["fdcId", "description", "dataType", "publishedDate", "category", "nutrients", "portions"],
        "properties": {
          "fdcId": { "type": "integer" },
          "description": { "type": "string" },
          "dataType": { "type": "string" },
          "publishedDate": { "type": ["string", "null"], "format": "date" },
          "category": {
            "oneOf": [
              { "$ref": "#/components/schemas/CategoryInfo" },
              { "type": "null" }
            ]
          },
          "canonicalBaseName": { "type": "string", "description": "Canonical base name (broad identity)" },
          "canonicalBaseSlug": { "type": "string", "description": "URL-safe slug of canonical base name" },
          "canonicalSpecificName": { "type": "string", "description": "Canonical specific name (subtype)" },
          "canonicalSpecificSlug": { "type": "string", "description": "URL-safe slug of canonical specific name" },
          "nutrients": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/NutrientInfo" }
          },
          "portions": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/PortionInfo" }
          }
        }
      },
      "NutrientInfo": {
        "type": "object",
        "required": ["nutrientId", "name", "unit", "amount"],
        "properties": {
          "nutrientId": { "type": "integer" },
          "name": { "type": "string" },
          "unit": { "type": "string" },
          "amount": { "type": "number" }
        }
      },
      "NutrientListItem": {
        "type": "object",
        "required": ["nutrientId", "name", "unit", "rank", "isEnergy"],
        "properties": {
          "nutrientId": { "type": "integer" },
          "name": { "type": "string" },
          "unit": { "type": "string" },
          "rank": { "type": ["integer", "null"] },
          "isEnergy": { "type": "boolean" }
        }
      },
      "PortionInfo": {
        "type": "object",
        "required": ["gramWeight", "amount", "unit", "modifier"],
        "properties": {
          "gramWeight": { "type": "number" },
          "amount": { "type": ["number", "null"] },
          "unit": { "type": ["string", "null"] },
          "modifier": { "type": ["string", "null"] }
        }
      },
      "CategoryInfo": {
        "type": "object",
        "required": ["categoryId", "name"],
        "properties": {
          "categoryId": { "type": "integer" },
          "name": { "type": "string" }
        }
      },
      "CategoryWithCount": {
        "type": "object",
        "required": ["categoryId", "name", "foodCount"],
        "properties": {
          "categoryId": { "type": "integer" },
          "name": { "type": "string" },
          "foodCount": { "type": "integer" }
        }
      },
      "PaginatedFoods": {
        "type": "object",
        "required": ["items", "total", "page", "pageSize"],
        "properties": {
          "items": { "type": "array", "items": { "$ref": "#/components/schemas/FoodListItem" } },
          "total": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        }
      },
      "PaginatedNutrients": {
        "type": "object",
        "required": ["items", "total", "page", "pageSize"],
        "properties": {
          "items": { "type": "array", "items": { "$ref": "#/components/schemas/NutrientListItem" } },
          "total": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        }
      },
      "CategoriesResponse": {
        "type": "object",
        "required": ["categories"],
        "properties": {
          "categories": { "type": "array", "items": { "$ref": "#/components/schemas/CategoryInfo" } }
        }
      },
      "CategoriesWithCountResponse": {
        "type": "object",
        "required": ["categories"],
        "properties": {
          "categories": { "type": "array", "items": { "$ref": "#/components/schemas/CategoryWithCount" } }
        }
      },
      "IngredientListItem": {
        "type": "object",
        "required": ["canonicalId", "ingredientName", "ingredientSlug", "syntheticFdcId", "frequency", "fdcCount", "hasNutrients"],
        "properties": {
          "canonicalId": { "type": "string", "format": "uuid" },
          "ingredientName": { "type": "string", "description": "Canonical ingredient name from recipe corpus" },
          "ingredientSlug": { "type": "string", "description": "URL-safe slug" },
          "syntheticFdcId": { "type": ["integer", "null"], "description": "Synthetic FDC ID (9,000,000+ range)" },
          "frequency": { "type": "integer", "description": "Recipe usage frequency" },
          "fdcCount": { "type": "integer", "description": "Number of mapped FDC foods" },
          "hasNutrients": { "type": "boolean", "description": "Whether nutrient boundaries have been computed" }
        }
      },
      "IngredientNutrient": {
        "type": "object",
        "required": ["nutrientId", "name", "unit", "median", "p10", "p90", "p25", "p75", "min", "max", "nSamples"],
        "properties": {
          "nutrientId": { "type": "integer" },
          "name": { "type": "string" },
          "unit": { "type": "string" },
          "median": { "type": "number", "description": "Median value per 100g across member foods" },
          "p10": { "type": ["number", "null"], "description": "10th percentile (null if n < 3)" },
          "p90": { "type": ["number", "null"], "description": "90th percentile (null if n < 3)" },
          "p25": { "type": ["number", "null"], "description": "25th percentile (null if n < 3)" },
          "p75": { "type": ["number", "null"], "description": "75th percentile (null if n < 3)" },
          "min": { "type": "number", "description": "Minimum observed value" },
          "max": { "type": "number", "description": "Maximum observed value" },
          "nSamples": { "type": "integer", "description": "Number of member foods with this nutrient" }
        }
      },
      "IngredientAlias": {
        "type": "object",
        "required": ["aliasNorm", "aliasCount", "aliasSource"],
        "properties": {
          "aliasNorm": { "type": "string", "description": "Normalized alias text" },
          "aliasCount": { "type": "integer", "description": "Usage frequency of this alias" },
          "aliasSource": { "type": "string", "description": "Source of the alias (e.g. 'recipe_corpus', 'manual')" }
        }
      },
      "IngredientMemberFood": {
        "type": "object",
        "required": ["fdcId", "description", "dataType", "membershipReason"],
        "properties": {
          "fdcId": { "type": "integer" },
          "description": { "type": "string" },
          "dataType": { "type": ["string", "null"] },
          "membershipReason": { "type": "string", "description": "Why this food was mapped (e.g. 'lexical:segment_primary')" }
        }
      },
      "IngredientDetail": {
        "type": "object",
        "required": ["canonicalId", "ingredientName", "ingredientSlug", "syntheticFdcId", "frequency", "fdcCount", "canonicalRank", "nutrients", "aliases", "memberFoods"],
        "properties": {
          "canonicalId": { "type": "string", "format": "uuid" },
          "ingredientName": { "type": "string" },
          "ingredientSlug": { "type": "string" },
          "syntheticFdcId": { "type": ["integer", "null"] },
          "frequency": { "type": "integer", "description": "Recipe usage frequency" },
          "fdcCount": { "type": "integer", "description": "Number of mapped FDC foods" },
          "canonicalRank": { "type": "integer", "description": "Rank by frequency in recipe corpus" },
          "nutrients": { "type": "array", "items": { "$ref": "#/components/schemas/IngredientNutrient" } },
          "aliases": { "type": "array", "items": { "$ref": "#/components/schemas/IngredientAlias" }, "description": "Alternative names for this ingredient" },
          "memberFoods": { "type": "array", "items": { "$ref": "#/components/schemas/IngredientMemberFood" }, "description": "FDC foods mapped to this ingredient" }
        }
      },
      "ResolvedIngredient": {
        "type": "object",
        "required": ["input", "match"],
        "properties": {
          "input": { "type": "string", "description": "Original input string" },
          "match": {
            "oneOf": [
              {
                "type": "object",
                "required": ["ingredientName", "ingredientSlug", "canonicalId", "syntheticFdcId", "frequency", "fdcCount", "method", "confidence", "nutrients"],
                "properties": {
                  "ingredientName": { "type": "string" },
                  "ingredientSlug": { "type": "string" },
                  "canonicalId": { "type": "string", "format": "uuid" },
                  "syntheticFdcId": { "type": ["integer", "null"] },
                  "frequency": { "type": "integer" },
                  "fdcCount": { "type": "integer" },
                  "method": { "type": "string", "enum": ["direct", "alias", "fuzzy"], "description": "How the match was found: direct slug, alias lookup, or trigram fuzzy" },
                  "confidence": { "type": "number", "minimum": 0, "maximum": 1, "description": "Match confidence (1.0 for direct/alias, similarity score for fuzzy)" },
                  "nutrients": { "type": "array", "items": { "$ref": "#/components/schemas/IngredientNutrient" } }
                }
              },
              { "type": "null" }
            ],
            "description": "Resolved canonical ingredient with nutrients, or null if unresolved"
          }
        }
      },
      "ResolveResponse": {
        "type": "object",
        "required": ["results", "resolved", "unresolved"],
        "properties": {
          "results": { "type": "array", "items": { "$ref": "#/components/schemas/ResolvedIngredient" } },
          "resolved": { "type": "integer", "description": "Count of successfully resolved ingredients" },
          "unresolved": { "type": "integer", "description": "Count of ingredients that could not be matched" }
        }
      },
      "PaginatedIngredients": {
        "type": "object",
        "required": ["items", "total", "page", "pageSize"],
        "properties": {
          "items": { "type": "array", "items": { "$ref": "#/components/schemas/IngredientListItem" } },
          "total": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        }
      },
      "CanonicalListItem": {
        "type": "object",
        "required": ["canonicalId", "canonicalSlug", "canonicalName", "foodCount"],
        "properties": {
          "canonicalId": { "type": "integer", "description": "Synthetic FDC ID (9,200,000+)" },
          "canonicalSlug": { "type": "string", "description": "URL-safe slug" },
          "canonicalName": { "type": "string", "description": "Human-readable canonical name" },
          "foodCount": { "type": "integer", "description": "Number of source foods in this aggregate" }
        }
      },
      "CanonicalNutrient": {
        "type": "object",
        "required": ["nutrientId", "name", "unit", "median", "p5", "p95", "min", "max", "sampleCount"],
        "properties": {
          "nutrientId": { "type": "integer" },
          "name": { "type": "string" },
          "unit": { "type": "string" },
          "median": { "type": "number", "description": "Median value per 100g across source foods" },
          "p5": { "type": ["number", "null"], "description": "5th percentile (null if n < 3)" },
          "p95": { "type": ["number", "null"], "description": "95th percentile (null if n < 3)" },
          "min": { "type": "number" },
          "max": { "type": "number" },
          "sampleCount": { "type": "integer", "description": "Number of foods with this nutrient" }
        }
      },
      "CanonicalSource": {
        "type": "object",
        "required": ["fdcId", "description", "dataType"],
        "properties": {
          "fdcId": { "type": "integer" },
          "description": { "type": "string" },
          "dataType": { "type": "string" }
        }
      },
      "CanonicalDetail": {
        "type": "object",
        "required": ["canonicalId", "canonicalSlug", "canonicalName", "level", "foodCount", "dataTypes", "representativeFdcId", "nutrients", "sources"],
        "properties": {
          "canonicalId": { "type": "integer" },
          "canonicalSlug": { "type": "string" },
          "canonicalName": { "type": "string" },
          "level": { "type": "string" },
          "foodCount": { "type": "integer" },
          "dataTypes": { "type": "array", "items": { "type": "string" } },
          "representativeFdcId": { "type": ["integer", "null"] },
          "nutrients": { "type": "array", "items": { "$ref": "#/components/schemas/CanonicalNutrient" } },
          "sources": { "type": "array", "items": { "$ref": "#/components/schemas/CanonicalSource" } }
        }
      },
      "PaginatedCanonicals": {
        "type": "object",
        "required": ["items", "total", "page", "pageSize"],
        "properties": {
          "items": { "type": "array", "items": { "$ref": "#/components/schemas/CanonicalListItem" } },
          "total": { "type": "integer" },
          "page": { "type": "integer" },
          "pageSize": { "type": "integer" }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "required": ["error"],
        "properties": {
          "error": {
            "type": "object",
            "required": ["code", "message"],
            "properties": {
              "code": { "type": "string", "enum": ["BAD_REQUEST", "UNAUTHORIZED", "NOT_FOUND", "INTERNAL_ERROR"] },
              "message": { "type": "string" },
              "details": { "type": "object" }
            }
          }
        }
      }
    }
  }
}
